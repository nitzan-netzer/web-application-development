{
  "swagger": "2.0",
  "info": {
    "description": "This is the API documentation for the user registration endpoint of our application.",
    "version": "1.0.0",
    "title": "User Registration API",
    "contact": {
      "email": "support@moonactive.com"
    }
  },
  "host": "localhost:3001",
  "basePath": "/api",
  "tags": [
    {
      "name": "auth",
      "description": "Authentication related endpoints"
    }
  ],
  "schemes": [
    "http"
  ],
  "paths": {
    "/auth/register": {
      "post": {
        "tags": [
          "auth"
        ],
        "summary": "Register a new user",
        "description": "This endpoint allows a new user to register by providing necessary details.",
        "operationId": "registerUser",
        "consumes": [
          "application/json"
        ],
        "produces": [
          "application/json"
        ],
        "parameters": [
          {
            "in": "body",
            "name": "body",
            "description": "User object that needs to be registered",
            "required": true,
            "schema": {
              "type": "object",
              "required": [
                "username",
                "email",
                "password"
              ],
              "properties": {
                "username": {
                  "type": "string",
                  "example": "Adirana"
                },
                "name": {
                  "type": "string",
                  "example": "Adirana"
                },
                "email": {
                  "type": "string",
                  "format": "email",
                  "example": "adirana@gmail.com"
                },
                "password": {
                  "type": "string",
                  "format": "password",
                  "example": "strongPassword"
                },
                "birthYear": {
                    "type": "number",
                    "example": 1990
                },
                "address": {
                    "type": "string",
                    "example": "1234 Main St"
                },
                "gender": {
                  "type": "string",
                  "enum": ["male", "female", "unknown"],
                  "example": "female"
                },
                "isSeller": {
                    "type": "boolean",
                    "example": false
                }
              }
            }
          }
        ],
        "responses": {
          "201": {
            "description": "User successfully registered",
            "schema": {
              "type": "object",
              "properties": {
                "id": {
                  "type": "string"
                },
                "username": {
                  "type": "string"
                },
                "email": {
                  "type": "string"
                }
              }
            }
          },
          "400": {
            "description": "Invalid input",
            "schema": {
              "type": "object",
              "properties": {
                "message": {
                  "type": "string"
                }
              },
              "example": {
                "application/json": {
                  "message": "Invalid input data"
                }
              }
            }
          },
          "409": {
            "description": "Conflict - User already exists",
            "schema": {
              "type": "object",
              "properties": {
                "message": {
                  "type": "string"
                }
              },
              "example": {
                "application/json": {
                  "message": "User already exists"
                }
              }
            }
          }
        }
      }
    },
    "/auth/login": {
      "post": {
        "tags": [
          "auth"
        ],
        "summary": "login an existing user",
        "description": "This endpoint allows an existing user to login by providing necessary details.",
        "operationId": "registerUser",
        "consumes": [
          "application/json"
        ],
        "produces": [
          "application/json"
        ],
        "parameters": [
          {
            "in": "body",
            "name": "body",
            "description": "User object that needs to be registered",
            "required": true,
            "schema": {
              "type": "object",
              "required": [
                "username",
                "email",
                "password"
              ],
              "properties": {
                "username": {
                  "type": "string"
                },
                "email": {
                  "type": "string",
                  "format": "email"
                },
                "password": {
                  "type": "string",
                  "format": "password"
                }
              }
            }
          }
        ],
        "responses": {
          "201": {
            "description": "User successfully logged in",
            "schema": {
              "type": "object",
              "properties": {
                "id": {
                  "type": "string"
                },
                "username": {
                  "type": "string"
                },
                "email": {
                  "type": "string"
                }
              }
            }
          },
          "400": {
            "description": "Invalid input",
            "schema": {
              "type": "object",
              "properties": {
                "message": {
                  "type": "string"
                }
              },
              "example": {
                "application/json": {
                  "message": "Invalid input data"
                }
              }
            }
          },
          "409": {
            "description": "Conflict - User can't login",
            "schema": {
              "type": "object",
              "properties": {
                "message": {
                  "type": "string"
                }
              },
              "example": {
                "application/json": {
                  "message": "User already exists"
                }
              }
            }
          }
        }
      }
    },
    "/product/create": {
      "post": {
        "tags": [
          "product"
        ],
        "summary": "Create a product",
        "description": "This endpoint allows a user to create a product.",
        "operationId": "createProduct",
        "consumes": [
          "application/json"
        ],
        "produces": [
          "application/json"
        ],
        "parameters": [
          {
            "in": "body",
            "name": "body",
            "description": "Product object",
            "required": true,
            "schema": {
              "type": "object",
              "required": [
                "username",
                "email",
                "password"
              ],
              "properties": {
                "name": {
                  "type": "string"
                },
                "category": {
                  "type": "string"
                },
                "status": {
                  "type": "string"
                },
                "description": {
                  "type": "string"
                },
                "price": {
                  "type": "number"
                },
                "userId": {
                  "type": "string"
                }
              }
            }
          },
          {
            "in": "header",
            "name": "x-auth-token",
            "description": "User token",
            "required": true,
            "type": "string"
          }
        ],
        "responses": {
          "201": {
            "description": "User successfully registered",
            "schema": {
              "type": "object",
              "properties": {
                "id": {
                  "type": "string"
                },
                "username": {
                  "type": "string"
                },
                "email": {
                  "type": "string"
                }
              }
            }
          },
          "400": {
            "description": "Invalid input",
            "schema": {
              "type": "object",
              "properties": {
                "message": {
                  "type": "string"
                }
              },
              "example": {
                "application/json": {
                  "message": "Invalid input data"
                }
              }
            }
          },
          "409": {
            "description": "Conflict - User already exists",
            "schema": {
              "type": "object",
              "properties": {
                "message": {
                  "type": "string"
                }
              },
              "example": {
                "application/json": {
                  "message": "User already exists"
                }
              }
            }
          }
        }
      }
    },
    "/product/update": {
      "post": {
        "tags": [
          "product"
        ],
        "summary": "Update a product",
        "description": "This endpoint allows a user to Update a product.",
        "operationId": "updateProduct",
        "consumes": [
          "application/json"
        ],
        "produces": [
          "application/json"
        ],
        "parameters": [
          {
            "in": "body",
            "name": "body",
            "description": "Product object",
            "required": true,
            "schema": {
              "type": "object",
              "required": [
                "username",
                "email",
                "password"
              ],
              "properties": {
                "name": {
                  "type": "string"
                },
                "category": {
                  "type": "string"
                },
                "status": {
                  "type": "string"
                },
                "description": {
                  "type": "string"
                },
                "price": {
                  "type": "number"
                }
              }
            }
          },
          {
            "in": "header",
            "name": "x-auth-token",
            "description": "User token",
            "required": true,
            "type": "string"
          }
        ],
        "responses": {
          "201": {
            "description": "User successfully registered",
            "schema": {
              "type": "object",
              "properties": {
                "id": {
                  "type": "string"
                },
                "username": {
                  "type": "string"
                },
                "email": {
                  "type": "string"
                }
              }
            }
          },
          "400": {
            "description": "Invalid input",
            "schema": {
              "type": "object",
              "properties": {
                "message": {
                  "type": "string"
                }
              },
              "example": {
                "application/json": {
                  "message": "Invalid input data"
                }
              }
            }
          },
          "409": {
            "description": "Conflict - User already exists",
            "schema": {
              "type": "object",
              "properties": {
                "message": {
                  "type": "string"
                }
              },
              "example": {
                "application/json": {
                  "message": "User already exists"
                }
              }
            }
          }
        }
      }
    }
  }
}
